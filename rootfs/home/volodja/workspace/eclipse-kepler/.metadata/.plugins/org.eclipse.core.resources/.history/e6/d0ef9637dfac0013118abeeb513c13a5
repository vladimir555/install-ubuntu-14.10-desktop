#include <iostream>
#include <string>
#include <typeinfo>


using namespace std;


void func(string s) {
    cout << s << endl;
}


int main() {
    cout << string(typeid(5.5).name()) << endl;

    cout << typeid((
            [&]() -> string {
                return string("lambda").c_str();
            })
    ).name() << endl;

    return 0;
}
