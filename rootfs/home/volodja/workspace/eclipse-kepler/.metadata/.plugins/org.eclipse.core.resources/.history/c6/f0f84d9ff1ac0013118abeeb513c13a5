#include <iostream>
#include <string>
#include <typeinfo>
#include <algorithm>
#include <vector>
#include "stopwatch.h"


using namespace std;


const string str = "jskjflksdlfjslflsjflksjlfkjslfkjsldfkjlskfjlskjflksjflksjlfkjsldfkjsldkfjlksjflksjlfksjdlfkjsldfkjslkfjlskdfjlskdfjlskjdflskdjflskdjflskdjflskdjflksjdlfksjdlf";


const string f() {
    return str;
}


int main() {
    size_t length = 0;

    utility::Stopwatch stopwatch();

    for (size_t i = 0; i < 1000; i++) {
        length += f().size();
    }

    cout << stopwatch.getElapsedMicrosecond() << endl;

    return length;
}
