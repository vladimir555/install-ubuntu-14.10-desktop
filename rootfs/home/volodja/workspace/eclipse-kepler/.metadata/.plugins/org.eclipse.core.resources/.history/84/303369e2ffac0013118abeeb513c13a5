#include <iostream>
#include <string>
#include <typeinfo>
#include <algorithm>
#include <vector>
#include "stopwatch.h"


using namespace std;


vector<string> text;


vector<string>&& f() {
    return text;
}


struct A {

};


int main() {

    for (size_t n = 0; n < 1000; n++) {
        string s;
        for (size_t m = 0; m < 1000; m++) {
            char r   = char((float)rand() / RAND_MAX * 100) + 32;
            s       += r;
        }
        text.push_back(s);
    }

    size_t number = 0;

    cout << "start" << endl;
    utility::Stopwatch stopwatch;

    for (size_t i = 0; i < 1; i++) {
        for (size_t n = 0; n < 1000; n++) {
            for (size_t m = 0; m < 1000; m++) {
                number += f()[n][m];
            }
        }
    }

    cout << stopwatch.getElapsedMicrosecond() << endl;
    cout << number << endl;

    return number;
}
