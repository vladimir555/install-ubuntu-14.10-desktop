#include <iostream>
#include <string>
#include <typeinfo>


using namespace std;


void func(string s) {
    cout << s << endl;
}


int main() {
    cout << string(typeid(string()).name()) << endl;

    cout << typeid((
            []() -> const string {
                const string result = "qwert";
                cout << "lambda retur: " << result << endl;
                return string("lambda").c_str();
            })
    ).name() << endl;

    return 0;
}
