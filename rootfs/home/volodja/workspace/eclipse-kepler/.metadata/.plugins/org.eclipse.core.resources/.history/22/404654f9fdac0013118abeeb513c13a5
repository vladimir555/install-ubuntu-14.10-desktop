#include <iostream>
#include <string>
#include <typeinfo>
#include <algorithm>
#include <vector>
#include "stopwatch.h"


using namespace std;


vector<string> text;


vector<string> f() {
    return text;
}


struct A {

};


int main() {


    for (size_t n = 0; n < 1000; n++) {
        string s;
        for (size_t m = 0; n < 1000; n++) {
            s += char(rand() / RAND_MAX * 100) + 32;
        }
        text.push_back(s);
    }

    size_t length = 0;

    utility::Stopwatch stopwatch;

    for (size_t i = 0; i < 100; i++) {
        length += f().size();
    }

    cout << stopwatch.getElapsedMicrosecond() << endl;

    return length;
}
