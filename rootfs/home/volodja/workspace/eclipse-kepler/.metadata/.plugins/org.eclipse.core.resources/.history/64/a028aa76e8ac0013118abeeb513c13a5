#include <iostream>
#include <string>
#include <typeinfo>
#include <algorithm>
#include <vector>


using namespace std;


void func(const string &s) {
    cout << s << endl;
}


class A {
public:
    A() {
    }

   ~A(){
    }
};


int main() {

    func (
        []() -> string {
            string result = "qwert";
            cout << "lambda: " << result << endl;
            return std::move(result);
        }());

    return 0;
}
