<prefwindow id="fvdsd_settings_dialog"
	title="&txt.dialog.title;"
	buttons="accept,extra1"
	buttonlabelextra1="&txt.dialog.button_get_premium_for_free.title;"	
	buttonlabelaccept="&txt.dialog.button_ok.title;"
	buttonlabelcancel="&txt.dialog.button_cancel.title;"
  ondialogextra1="fvds.showShareForPremium();"
	ondialogaccept="return fvds.ok(event, true);"
	ondialogcancel="return fvds.ok(event, false);"
	xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
	xmlns:html="http://www.w3.org/1999/xhtml"
	>

	<script type="application/x-javascript" src="chrome://fvd.speeddial/content/fvd_settings.js" />
	<script type="application/x-javascript" src="chrome://fvd.speeddial/content/include/html_components/colorpicker/include.js" />
	<script type="application/x-javascript" src="chrome://fvd.speeddial/content/include/sd/background_adjuster.js"/>
	<script type="application/x-javascript" src="chrome://fvd.speeddial/content/include/tools/misc.js"/>
	<script type="application/x-javascript" src="chrome://fvd.speeddial/content/include/sd/background_adjuster.js"></script>
	<script type="application/x-javascript" src="chrome://fvd.speeddial/content/include/tools/popup_image_helper.js"></script>
	<script type="application/x-javascript" src="chrome://fvd.speeddial/content/include/sd/import_export.js"></script>
	
	<popupset>
		<panel id="colorPicker"
		hidden="false"
		type="arrow"
	    orient="vertical" 
	    ignorekeys="true"	     
		
		>
			<vbox>
				<iframe id="colorPickerFrame" type="chrome" src=""></iframe>
			</vbox>
			
		</panel>
		
		<panel id="__popupImageHelper"
		hidden="false"
		type="arrow"
	    orient="vertical" 
	    ignorekeys="true"	     
		
		>
			<vbox>
				<image class="inner"/>
			</vbox>
			
		</panel>
		
		<panel id="donatePanel"
		hidden="false"
		type="arrow"
	    orient="vertical" 
	    ignorekeys="true"	     
		
		>
			<vbox>
				
				<description>&donate_message;</description>					
				
				<hbox pack="center">
					<image tooltiptext="&donate_button.text;" class="donateButton" onclick="fvds.donate()" />					
				</hbox>
					
			</vbox>
			
		</panel>
		
		<menupopup id="suggestionsPopup">  
			<menuitem label="&suggestions_popup.submit_suggestion;" oncommand="fvds.submitSuggestion()"/>  
			<menuitem label="&suggestions_popup.report_bug;" oncommand="fvds.reportBug()"/>  
		</menupopup>
		
	</popupset>
	

	
