		<tabbox>
			<tabs id="globalTabs">
				<tab label="&txt.global.tabs.general;" id="globalTabs_general" />
				<tab label="&txt.global.tabs.appearance;" id="globalTabs_appearance" />
			</tabs>
			<tabpanels>
				<tabpanel id="globalTabs_general">
				
					<vbox flex="2">
				
						<grid>
							<columns>
								<column flex="1"/>
								<column flex="2"/>
							</columns>
							<rows>
								
								<row>									
									<hbox flex="2"><checkbox label="&txt.group.sd.make_home_page;" id="sd_make_home_page" /><spacer flex="1" /></hbox>
									<hbox flex="2"><checkbox label="&txt.group.toolbar_elements.disable_plugins.title;" preference="fvd_sd_disable_plugins" /><spacer flex="1" /></hbox>
								</row>	
								<row>							
									
									<hbox flex="2"><checkbox label="&txt.group.sd.show_in_last_tab;" preference="fvd_sd_show_in_last_tab" /><spacer flex="1" /></hbox>							
								</row>
								
							</rows>
						</grid>
						
										
						<hbox flex="2">
							<groupbox flex="2">
								<caption label="&fvd_sd.group.default.title;" />
								<vbox>	
								
								<radiogroup id="sd_default_mode" preference="fvdsd_sd_display_type">
								  <radio id="sd_default_top_sites" value="top_sites" label="&fvd_sd.group.default.top_sites;"/>
								  <radio id="sd_default_most_visited" value="most_visited" label="&fvd_sd.group.default.most_visited;"/>
								  <radio id="sd_default_recently_closed" value="recently_closed" label="&fvd_sd.group.default.recently_closed;"/>
								  <radio value="last_selected" label="&fvd_sd.group.default.last_selected;"/>
								</radiogroup>
								
								</vbox>
							</groupbox>	
							
							<groupbox flex="2">
								<caption label="&fvd_sd.group.enable.title;" />
								<vbox>	
										
								  <checkbox id="enable_top_sites" oncommand="sdEnableClick()" label="&fvd_sd.group.default.top_sites;" preference="fvdsd_sd_enable_top_sites"/>
								  <checkbox id="enable_most_visited" oncommand="sdEnableClick()" label="&fvd_sd.group.default.most_visited;" preference="fvdsd_sd_enable_most_visited"/>
								  <checkbox id="enable_recently_closed" oncommand="sdEnableClick()" label="&fvd_sd.group.default.recently_closed;" preference="fvdsd_sd_enable_recently_closed"/>
								  <checkbox label="&fvd_sd.group.display_restore_previous_session;" preference="fvd_sd_display_restore_previous_session" />
								
								</vbox>
							</groupbox>	
							
						</hbox>
						
						<hbox flex="2">
						
							<groupbox flex="2">
								<caption label="&fvd_sd.group.blocked_domains.title;" />
								<hbox pack="center" align="center" flex="2">	
									<button oncommand="fvds.manageBlockedDomains()" label="&fvd_sd.group.blocked_domains.manage;"/>			
								</hbox>
							</groupbox>	
							
							<groupbox flex="2">
								<caption label="&fvd_sd.group.dials_opening.title;" />
								
								<radiogroup preference="fvdsd_sd_open_links_type">
								  <radio value="current_tab" label="&txt.group.dials_opening.current_tab.title;"/>
								  <radio value="new_tab" label="&txt.group.dials_opening.new_tab.title;"/>
								  <radio value="new_tab_passive" label="&txt.group.dials_opening.new_tab_passive.title;"/>
								</radiogroup>
								
							</groupbox>	
						
						</hbox>	

						<hbox flex="2">
						
							<groupbox flex="2" hidden="true">
								<caption><checkbox label="&fvd_sd.group.hot_key.use_hot_key;" id="useHotKey" preference="fvd_sd_use_hot_key" oncommand="fvds.refreshHotKeyActivity()" /></caption>
								<hbox pack="center" align="center" flex="2">	
									<vbox flex="2">
										<textbox emptyText="Click here, than press your Hotkey" id="hotkeyCode" onkeydown="fvds.hotKeyScan(event)" flex="2" preference="fvd_sd_hot_key" />
										<hbox>
											<label value="&fvd_sd.group.hot_key.apply_changes;"/>
											<label  value="&fvd_sd.group.hot_key.already_used;" id="hotKeyAlreadyUsed" hidden="true"/>
										</hbox>
									</vbox>
								</hbox>
							</groupbox>	
						
						</hbox>	
					
					</vbox>
				
				</tabpanel>
				
				<tabpanel id="globalTabs_appearance">
					
					<vbox flex="2">

						<grid>
							<columns>
								<column flex="1"/>
								<column flex="2"/>
							</columns>
							<rows>
								<row>
									<hbox flex="2"><checkbox label="&txt.group.hooks.about_blank.title;" preference="fvdsd_general_hook_about_blank" /><spacer flex="1" /></hbox>
									<hbox flex="2"><checkbox label="&txt.group.global_options.display_main_button;" id="fvdsd_display_main_button" /><spacer flex="1" /></hbox>									
								</row>	
								<row>
									<hbox flex="2"><checkbox label="&txt.group.sd.show_url_in_dial;" preference="fvd_sd_show_url_in_dial" /><spacer flex="1" /></hbox>
								  <hbox flex="2"><checkbox label="&txt.group.global_options.display_urlbar_button;" preference="fvd_sd_display_url_bar_button" /><spacer flex="1" /></hbox>										
								</row>	
								<row>		
									<hbox flex="2"><checkbox label="&txt.group.hooks.thumbs_icons_and_titles.title;" preference="fvd_sd_thumbs_icons_and_titles" /><spacer flex="1" /></hbox>
                  <hbox flex="2">
                    <checkbox label="&txt.group.global_options.show_clicks_and_quick_menu;" preference="fvd_sd_show_clicks_and_quick_menu" />
                    <vbox pack="center">
                      <hbox class="sdIcon question clickable imageHelper" helperImage="show_clicks_and_quick_menu" />
                    </vbox>   
                  </hbox> 									
								</row>		

								<row>
									<hbox flex="2"><checkbox label="&txt.group.sd.disable_custom_search;" preference="fvd_sd_disable_custom_search" /><spacer flex="1" /></hbox>
									
                  <hbox flex="2">
                    <checkbox label="&txt.group.top_sites.display_plus_cells.title;" preference="fvd_sd_display_plus_cells" />
                    <vbox pack="center">
                      <hbox class="sdIcon question clickable imageHelper" helperImage="display_plus_cells" />                   
                    </vbox>                   
                  </hbox>
  							</row>
								<row>
									<hbox flex="2">
										<checkbox label="&txt.group.top_sites.hide_cell_background.title;" preference="fvd_sd_hide_background" />
										<vbox pack="center">
											<hbox class="sdIcon question clickable imageHelper" helperImage="hide_background" />
										</vbox>		
									</hbox>	
									
									<checkbox label="&txt.group.top_sites.display_button_in_google_results.title;" preference="fvd_sd_display_button_in_google_results" />                  									
								</row>
								<row>
									<hbox flex="2">
										<checkbox label="&txt.group.global_options.show_in_context_menu;" preference="fvd_sd_show_in_context_menu" />
										<vbox pack="center">
											<hbox class="sdIcon question clickable imageHelper" helperImage="display_in_context_menu" />
										</vbox>	
									</hbox>
																		
									<checkbox hidden="true" label="&txt.group.top_sites.disable_like_button.title;" preference="fvd_sd_disable_like_button" />							
								</row>
								
								<!--
								<row>
									<checkbox label="&txt.group.global_options.display_search_preview;" preference="fvd_sd_display_search_preview" />
															
								</row>
								-->
								
								
							</rows>
						</grid>
					
						<hbox>
						
							<groupbox flex="2">
								<caption label="&fvd_sd.dials_opacity.title;" />
								<hbox flex="2">	
									<scale min="0" max="100" preference="fvd_sd_dials_opacity" flex="2" id="scale_fvd_sd_dials_opacity" />									
								</hbox>
								<hbox flex="2">
									<hbox flex="2">0%</hbox><hbox flex="2" style="text-align:center">50%</hbox><hbox flex="2" style="text-align: right">100%</hbox>
								</hbox>
							</groupbox>	
						
						</hbox>
						
						<hbox>
						
							<groupbox flex="2" id="groupBoxSetupCustomSize">
								<caption label="&fvd_sd.custom_dial_size.title;" />
								<hbox>									
									<scale min="150" id="customDialSizeScale" max="260" flex="2" />
																		
									<hbox align="center">
										<label id="customDialSize"/><hbox class="countMax">&fvd_sd.custom_dial_size.max_min_width;</hbox>	
									</hbox>									
								</hbox>
							</groupbox>	
						
							<groupbox flex="2" id="groupBoxSetupCustomSizeMostVisited">
								<caption label="&fvd_sd.custom_dial_size_mostvisited.title;" />
								<hbox>									
									<scale min="150" id="customDialSizeScaleMostVisited" max="260" flex="2" />
																		
									<hbox align="center">
										<label id="customDialSizeMostVisited"/><hbox class="countMax">&fvd_sd.custom_dial_size.max_min_width;</hbox>	
									</hbox>									
								</hbox>
							</groupbox>	
						
						
						
						</hbox>	
						
						<hbox>
						
							<groupbox flex="2">
								<caption label="&fvd_sd.scrolling;" />
								<hbox>
								
									<menulist  flex="2"  preference="fvd_sd_scrolling" crop="right">
										<menupopup>
											<menuitem label="&scrolling.vertical;" value="vertical"/>  
											<menuitem label="&scrolling.horizontal;" value="horizontal"/>  
										</menupopup>
									</menulist>									
															
								</hbox>
								<hbox id="themesHelpMessage">
									<label value="&themes_help.dont_forget_to_change;"/>
									<label value="&themes_help.font_colors;" onclick="goToTab( 'paneSdFontColors' )" class="linkedText"/>
									<label value="&themes_help.and_set_nice;"/>
									<label value="" id="themesHelpBackgrounName"/>
									<label class="linkedText" onclick="goToTab( 'paneSdBackground' )" value="&themes_help.background;"/> 
									<label value=":)"/>
								</hbox>
							</groupbox>	
						
						</hbox>
						
